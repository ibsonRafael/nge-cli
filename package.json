    "chalk": "^2.0.1",
    "conf": "^1.1.0",
    "figures": "^2.0.0",
    "nge-generator": "^2.1.3",
    "nge-generator-addon": "^1.1.2",
    "inquirer": "^3.0.6",
    "minimist": "^1.2.0",
    "update-notifier": "^2.2.0",
    "yeoman-environment": "^2.0.0"
